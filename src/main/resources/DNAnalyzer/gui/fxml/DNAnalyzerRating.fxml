<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import org.controlsfx.control.Rating?>
<?import javafx.scene.control.Label?>
<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DNAnalyzer.ui.gui.DNAnalyzerRatingFXMLController" spacing="10" alignment="CENTER" prefWidth="300" prefHeight="200">
    <children>
        <Label text="Rate DNAnalyzer" />
        <Rating fx:id="dnaRating" max="5" />
        <TextArea fx:id="dnaRatingTxt" prefRowCount="3" promptText="Leave a comment" />
        <Button text="Submit" onAction="#btnSubmitClicked" />
    </children>
</VBox>
